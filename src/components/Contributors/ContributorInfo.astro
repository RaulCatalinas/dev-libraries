---
import ImagePage from '../ImagePage.astro';

interface Props {
  contributorImageURL: string;
  contributorName: string;
}

const { contributorImageURL, contributorName } = Astro.props;
---

<ImagePage
  imageSrc={contributorImageURL}
  imageAlt={contributorName}
  imageSize={56}
  imageClass="rounded-full"
/>
